#!/bin/bash

scp *.py ve73yen@ppc801.mirz.uni-jena.de:~/

for i in `seq 1 19`;
do
  num=$(printf "%02d" $i)
  toRun="ssh -t ve73yen@ppc8$num.mirz.uni-jena.de 'python3 differenceMap_Beta1_n3_V16.py'"
  osascript -e "tell application \"Terminal\" to do script \"$toRun\""
done

ssh -t ve73yen@ppc820.mirz.uni-jena.de 'python3 findStartVals_n5_V1'
